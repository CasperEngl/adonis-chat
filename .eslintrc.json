{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
        "use": true
    },
    "env": {
      "browser": true
    },
    "plugins": ["react"],
    "rules": {
      "strict": "error",
      "no-shadow": 0,
      "func-names": 0,
      "no-console": 0,
      "consistent-return": 0,
      "react/prop-types": "error",
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0,
      "no-param-reassign": 0
    }
  }
  